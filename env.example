# Environment Variables for Underground Voices Frontend
# Copy this file to .env.local and update the values

# API Configuration
VITE_API_URL=http://localhost:3001/api

# Encryption Key (change this in production!)
VITE_ENCRYPTION_KEY=your-secure-encryption-key-here

# App Configuration
VITE_APP_NAME=Underground Voices
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_VOICE_HINTS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false
